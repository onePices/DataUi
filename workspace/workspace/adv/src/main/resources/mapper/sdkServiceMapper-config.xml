<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"  
"http://mybatis.org/dtd/mybatis-3-config.dtd">  
<configuration>
	<settings>
	    <setting name="logImpl" value="STDOUT_LOGGING" />
		<!-- 开启驼峰自动映射 -->
		<setting name="mapUnderscoreToCamelCase" value="true" />
		<!-- 二级缓存的总开关 -->
		<setting name="cacheEnabled" value="false" />
	</settings>
    
    <mappers>
		  <mapper resource="mapper/sdkservice/SysDeviceMapLogMapper.xml" />
		  <mapper resource="mapper/sdkservice/PlayTaskBasicsLogMapper.xml" />
		  <mapper resource="mapper/sdkservice/SysDeviceOnlineLogMapper.xml" />
		  <mapper resource="mapper/sdkservice/SysTaskExpireLogMapper.xml" />  
		  <mapper resource="mapper/sdkservice/TmlDeviceBasicMapper.xml" />
    </mappers>
</configuration>